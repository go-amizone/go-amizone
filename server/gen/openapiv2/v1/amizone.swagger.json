{
  "swagger": "2.0",
  "info": {
    "title": "Amizone API",
    "version": "0.7.0",
    "contact": {
      "name": "ditsuke",
      "url": "https://ditsuke.com",
      "email": "hello@ditsuke.com"
    },
    "license": {
